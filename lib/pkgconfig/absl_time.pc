prefix=/opt/anaconda3/envs/manim_proj
exec_prefix=${prefix}
libdir=/opt/anaconda3/envs/manim_proj/lib
includedir=/opt/anaconda3/envs/manim_proj/include

Name: absl_time
Description: Abseil time library
URL: https://abseil.io/
Version: 20250127
Requires: absl_base = 20250127, absl_civil_time = 20250127, absl_core_headers = 20250127, absl_int128 = 20250127, absl_raw_logging_internal = 20250127, absl_strings = 20250127, absl_time_zone = 20250127
Libs: -L${libdir}  -labsl_time 
Cflags: -I${includedir} -Wno-float-conversion -Wno-implicit-float-conversion -Wno-implicit-int-float-conversion -Wno-unknown-warning-option -DNOMINMAX
